    "precss": "^3.1.2",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "belle": "^4.0.0",
    "bootstrap": "^4.1.3",
    "js-cookie": "^2.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^5.2.0",
    "regenerator-runtime": "^0.13.7"
  }
